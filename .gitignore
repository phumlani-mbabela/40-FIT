# --------------------------------------------
# React / Node.js .gitignore
# --------------------------------------------

# Dependencies
/node_modules/

# Production build output
/build/
/dist/
/.astro/

# Local env files (never commit secrets)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Vite / Parcel / CRA cache
/.parcel-cache/
/.yarn/cache/
/.pnp/
/.pnp.js
.pnpm-debug.log

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*
*.log

# OS Generated files
.DS_Store
Thumbs.db

# IDE/project files
.vscode/
.idea/
*.swp
*.swo

# Testing coverage
/coverage/

# Storybook
/.storybook/
/out/
/storybook-static/

# ESLint cache
/.eslintcache

# Misc
*.tgz
*.tar
*.zip
*.bak
*.tmp

# Optional – if using Firebase / Supabase local emulators
/firebase-debug.log
/.firebase/
.supabase

# Optional – if using Docker
docker-compose.override.yml

# Optional – ignore compiled files for TypeScript
*.tsbuildinfo

# Local backend folders (if frontend & backend in same monorepo)
